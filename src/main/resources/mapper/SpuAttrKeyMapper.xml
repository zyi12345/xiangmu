        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.bwf.shop.admin.mapper.SpuAttrKeyMapper">
<select id="getAllSpuAttrKeyList" resultMap="SpuAttrKey">
    select * from spu_attr_key

</select>
